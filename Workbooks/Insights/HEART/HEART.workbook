                {
                  "id": "43be0fbf-f7c5-4822-a582-b59407774e59",
                  "version": "KqlParameterItem/1.0",
                  "name": "SegmentBy",
                  "type": 2,
                  "isRequired": true,
                  "query": "datatable(value:string, text:string)[\r\n'itemType', 'â None',\r\n'name', 'ð Activity',\r\n'cloud_RoleInstance', 'ð¥ï¸ Machine',\r\n'client_CountryOrRegion', 'ð Country or Region',\r\n'client_StateOrProvince', 'ð State or Province',\r\n'client_City', 'ð City',\r\n'client_Browser', 'ð Client Browser',\r\n'client_OS', 'ï¸ï¸ð¥ï¸ Client Operating System',\r\n'client_Model', 'ð± Client Model',\r\n'application_Version', 'âª Application Version',\r\n'operation_Name', 'âª Operation',\r\n]\r\n| union (union pageViews, customEvents\r\n| where timestamp >= ago(90d)\r\n| where name in ({TopStep}) or '*' in ({TopStep})\r\n| project customDimensions \r\n| summarize schema = buildschema(customDimensions) \r\n| mvexpand schema\r\n| extend e = extract(@'{\"(.+)\":.+}', 1, tostring(schema))\r\n| project text = strcat('âï¸ ', e), value = strcat('customDimensions[\"', e, '\"]'))",
                  "value": "itemType",
