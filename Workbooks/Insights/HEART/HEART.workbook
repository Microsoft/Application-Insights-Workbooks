              "version": "KqlItem/1.0",
              "query": "let tablePack = pageViews \r\n| where isnotempty(user_AuthenticatedId)\r\n| project array = pack_array(*), Dummy = 1\r\n;\r\npageViews\r\n| getschema \r\n| project ColumnName, ColumnOrdinal, Dummy = 1\r\n| join kind = inner (tablePack) on Dummy\r\n| project ColumnName, value = tostring(array[ColumnOrdinal])\r\n| summarize Complete=countif(value !in (\"\", \"not_specified\")), Rows=count() by ColumnName\r\n| extend Completion = Complete * 1.0 / Rows\r\n| project Status=case(Completion >= 0.90, 'âï¸ Good', 'â ï¸ Low Completion'), Attribute=ColumnName, Completion=coalesce(case(isnan(Completion), 0.0, Completion ), 0.0), Complete, Rows\r\n| order by Completion asc\r\n| where Attribute in (\"user_AuthenticatedId\", \"session_Id\", \"appName\", \"itemType\", \"timestamp\", \"operation_Id\")\r\n",
              "size": 1,
              "title": "pageViews Attribute Completion",
